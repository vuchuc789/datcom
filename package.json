{
  "name": "dat-com-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "type": "tsc",
    "prepare": "husky install",
    "dev:emulator": "concurrently -n \"firebase,next\" -p \"[{name}|{time}]\" -c \"yellow,blue\" -t \"HH:mm:ss.SSS\" \"firebase emulators:start\" \"wait-on tcp:9099 tcp:8080 tcp:9199 && NEXT_PUBLIC_EMULATOR=true next dev\"",
    "build:analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@firebase/app-types": "^0.8.1",
    "@mui/material": "^5.10.16",
    "@next/font": "^13.0.6",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.3.0",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-tools": "^11.16.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "typescript": "4.9.3",
    "vercel": "^28.7.2",
    "wait-on": "^6.0.1"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "7.x"
  }
}
